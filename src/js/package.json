{
    "name": "fusion-hawking",
    "version": "0.1.0",
    "description": "Lightweight SOME/IP stack for JavaScript/TypeScript (AUTOSAR R22-11)",
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "scripts": {
        "build": "tsc",
        "test": "node --test tests/test_codec.mjs tests/test_config.mjs tests/test_sd.mjs tests/test_cross_decode.mjs tests/test_tcp.mjs",
        "test:coverage": "c8 --reporter=html --reporter=text --output=../../logs/latest/coverage/js npm test",
        "clean": "node -e \"const fs=require('fs');fs.rmSync('dist',{recursive:true,force:true})\""
    },
    "keywords": [
        "someip",
        "automotive",
        "autosar",
        "ipc"
    ],
    "license": "MIT",
    "engines": {
        "node": ">=18.0.0"
    },
    "devDependencies": {
        "@types/node": "^20.0.0",
        "c8": "^9.0.0",
        "typescript": "^5.3.0"
    }
}